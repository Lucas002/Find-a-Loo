<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Find-a-Loo</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
	
	<splash platform="android" qualifier="port-hdpi" src="res/screen/android/drawable-hpdi/fal_logo9.png" />
	<splash platform="android" qualifier="port-mdpi" src="res/screen/android/drawable-mpdi/fal_logo9.png" />
	<splash platform="android" qualifier="port-xhdpi" src="res/screen/android/drawable-xhpdi/fal_logo9.png" />
	<splash platform="android" qualifier="port-xxdpi" src="res/screen/android/drawable-xxhpdi/fal_logo9.png" />
	<splash platform="android" qualifier="port-xxxdpi" src="res/screen/android/drawable-xxxhpdi/fal_logo9.png" />

	

	<platform name="ios">    
		<splash src="res/screen/ios/Default~iphone.png" width="320" height="480"/>
		<splash src="res/screen/ios/Default@2x~iphone.png" width="640" height="960"/>

		<splash src="res/screen/ios/Default-Portrait~ipad.png" width="768" height="1004"/>
		<splash src="res/screen/ios/Default-Portrait@2x~ipad.png" width="1536" height="2008"/>

		
		<splash src="res/screen/ios/Default-568h@2x~iphone.png" width="640" height="1136"/>
		<splash src="res/screen/ios/Default-667h.png" width="750" height="1334"/>
		<splash src="res/screen/ios/Default-736h.png" width="1224" height="2208"/>

	</platform>

	<splash platform="windows" src="res/screen/windows/splashImage.png" width="1152" height="1920"/>
	
	<plugin name="cordova-plugin-splashscreen" source="npm" />
	
	
	<splash src="splash.png" />
	
	<preference name="SplashScreen" value="screen"/>
	
	
</widget>
